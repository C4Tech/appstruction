<fieldset>
    <input type="number" placeholder="Quantity" id="concrete-quantity-{{cid}}" name="quantity" value="{{row.quantity}}" class="concrete field quantity"></input>
    <input type="number" placeholder="Depth" id="concrete-depth-{{cid}}" name="depth" value="{{row.depth}}" class="concrete field depth"></input>
    <input type="number" placeholder="Width" id="concrete-width-{{cid}}" name="width" value="{{row.width}}" class="concrete field width"></input>
    <input type="number" placeholder="Length" id="concrete-length-{{cid}}" name="length" value="{{row.length}}" class="concrete field length"></input>
    <input type="number" placeholder="Price" id="concrete-price-{{cid}}" name="price" value="{{row.price}}" class="concrete field price"></input>
    <input type="number" placeholder="Tax rate" id="concrete-tax-{{cid}}" name="tax" value="{{row.tax}}" class="concrete field tax"></input>
</fieldset>
