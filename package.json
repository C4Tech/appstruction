{
  "name": "appstruction",
  "description": "Estimate cement pouring jobs with ease.",
  "author": "C4 Tech and Design",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": "*"
  },
  "scripts": {
    "build": "brunch build",
    "compile": "npm run prepare-phonegap && npm run phonegap",
    "mocha": "./node_modules/.bin/mocha-casperjs",
    "phonegap": "phonegap prepare android && phonegap build android",
    "prepare": "npm run prepare-node && npm run prepare-bower",
    "prepare-node": "npm prune && npm install",
    "prepare-bower": "bower prune && bower install",
    "prepare-phonegap": "phonegap platform add android",
    "recompile": "npm run build && npm run phonegap",
    "serve": "npm run server &",
    "server": "./node_modules/.bin/http-server www",
    "start": "npm run prepare && npm run build",
    "start-over": "npm run torch && npm run start",
    "run-device": "phonegap run android --device",
    "test": "npm run serve && npm run mocha",
    "torch": "rm -rf ./bower_components ./node_modules",
    "update": "npm run update-node && npm run update-bower",
    "update-node": "npm prune && npm update",
    "update-bower": "bower prune && bower update",
    "watch": "brunch watch",
    "watch-server": "brunch watch --server"
  },
  "dependencies": {
    "auto-reload-brunch": "^1.8.0",
    "autoprefixer-brunch": "^1.7.0",
    "clean-css-brunch": "^1.7.0",
    "coffee-script-brunch": "^1.7.0",
    "coffeelint-brunch": "^1.7.0",
    "css-brunch": "^1.7.0",
    "javascript-brunch": "^1.7.0",
    "jsenv-brunch": "^1.4.2",
    "less-brunch": "^1.7.0",
    "react-coffee-brunch": "^1.7.1",
    "uglify-js-brunch": "^1.7.0"
  },
  "devDependencies": {
    "casper-chai": "^0.2.1",
    "casperjs": "^1.1.0-beta3",
    "chai": "^3.2.0",
    "coffee-script": "^1.9.3",
    "http-server": "^0.8.0",
    "mocha": "^2.2.5",
    "mocha-casperjs": "^0.5.4",
    "phantomcss": "^0.10.2",
    "resemblejs": "^1.3.0"
  }
}
